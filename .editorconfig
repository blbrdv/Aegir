# top-most EditorConfig file
root = true

[*]
indent_style = space
indent_size = 4
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = false
insert_final_newline = true

[*.{csproj,Config,yaml,yml,json}]
indent_size = 2

[*.{sln,cmd,ps1}]
end_of_line = crlf

[*.cs]
dotnet_diagnostic.IDE0051.severity = none
